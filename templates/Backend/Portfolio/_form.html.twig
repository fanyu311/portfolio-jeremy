{{ form_start(form, {'attr': {'class': 'form'}}) }}
{{ form_row(form.title) }}
<div class="row">
	<div>
		{{ form_row(form.metaTitle) }}
	</div>
	<div>{{ form_row(form.metaDescription) }}</div>
</div>

{{ form_row(form.content) }}

<div class="text-center">
	<button class="btn btn-primary" type="submit">{{ buttonLabel|default('Save') }}</button>
</div>

{{ form_row(form._token) }}

{{ form_end(form, {render_rest:false}) }}
