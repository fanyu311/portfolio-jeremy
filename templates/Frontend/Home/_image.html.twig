<div class="image-body">
	<div class="image-container">
		{% for photo in portfolios %}
			{% if '.mp4' not in photo.imageName %}
				<div class="item active" style="background-image: url('{{ vich_uploader_asset(photo)}}')">
					<div class="shadow"></div>
					<div class="image-content">
						<div class="text">
							<div class="title">{{ photo.title}}</div>
							{# <div class="sub">{{ photo.content}}</div> #}
						</div>
					</div>
				</div>
			{% endif %}
		{% endfor %}
	</div>

</div>


<div class="swiper banner-slider">

	<div class="swiper-wrapper">
		<hr>
		{% for photo in portfolios %}
			{% if '.mp4' not in photo.imageName %}
				<div class="swiper-slide">
					<img src="{{ vich_uploader_asset(photo) }}" alt="photo">
				</div>
			{% endif %}
		{% endfor %}
	</div>
	<div class="swiper-pagination"></div>
